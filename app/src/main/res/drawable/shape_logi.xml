<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="10dp"
        android:height="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#1aff570b" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item
        android:width="6dp"
        android:top="2dp"
        android:left="2dp"
        android:height="6dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffff570b" />
            <corners android:radius="20dp" />
        </shape>
    </item>
</layer-list>